<!DOCTYPE html>
<html lang="en">
<head>
    <nav class="navbar navbar-dark bg-primary">
        <button action="/"  method="get" type="submit" class="btn btn-link-dark">Home</button>
        <button type="submit" class="btn btn-link-dark">Funcionários</button>
        <button type="submit" class="btn btn-link-dark">Passagem</button>
        <button type="submit" class="btn btn-link-dark">Ônibus</button>
        <button type="submit" class="btn btn-link-dark">Empresas</button>
        <button type="submit" class="btn btn-link-dark">Rotas</button>
      </nav>
</head>
<body>
    <link href="https://cdn.jsdelivr.net/npm/bootstrap@5.0.2/dist/css/bootstrap.min.css" rel="stylesheet" integrity="sha384-EVSTQN3/azprG1Anm3QDgpJLIm9Nao0Yz1ztcQTwFspd3yD65VohhpuuCOmLASjC" crossorigin="anonymous">
    <h1 class="text-center title-1"> Lista de Funcionários </h1>
        <table class="table">
            <thead class="table-dark">
              <tr>
                <th scope="col">Nome</th>
                <th scope="col">CPF</th>
                <th scope="col">Cargo</th>
                <th scope="col">Telefone</th>
                <th scope="col">Endereço</th>
              </tr>
            </thead>
            <% retornolist.forEach(function(user){ %>
                <tbody>
                    <tr>
                    <td><%= user.nomeFuncionario %></td>
                    <td><%= user.cpf %></td>
                    <td><%= user.cargo %></td>
                    <td> <%= user.telefone %></td>
                    <td><%= user.endereco %></td>
                    </tr>
                </tbody>
            <%})%>
        </table>
    
</body>
</html>
